function briefRotTest()
% A test script performing BRIEF match using the given functions
im1 = imread('../data/model_chickenbroth');

perf = zeroes
for i = 1 : 36
   imr = imrotate(im1,i*10);
   [locs1, desc1] = briefLite(im1);
   [locs2, desc2] = briefLite(imr);

   [matches] = briefMatch(desc1, desc2,0.1);
   plotMatches(im1, imr, matches, locs1, locs2);

   
end

end